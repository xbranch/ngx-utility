{
  "name": "ngx-utility",
  "version": "14.0.1-snapshot.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-path ./docs --base-href /ngx-utility/",
    "test": "ng test",
    "lint": "ng lint",
    "build:core": "ng build core --configuration production",
    "build:files": "ng build files --configuration production",
    "build:dates": "ng build dates --configuration production",
    "publish:core": "npm publish ./dist/core --access public",
    "publish:files": "npm publish ./dist/files --access public",
    "publish:dates": "npm publish ./dist/dates --access public",
    "build:lib": "npm run build:core && npm run build:files && npm run build:dates",
    "update:docs": "npm run build && git add -A docs/ && git commit -m 'Update docs app'",
    "release": "ngitflow release start && npm run update:docs && ngitflow release finish",
    "release:major": "ngitflow release start major && npm run update:docs && ngitflow release finish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/router": "^15.2.2",
    "highlight.js": "^11.7.0",
    "moment": "^2.29.4",
    "ngx-highlightjs": "^8.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular-devkit/core": "^15.2.2",
    "@angular/cli": "^15.2.2",
    "@angular/compiler-cli": "^15.2.2",
    "@angular/language-service": "^15.2.2",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.14.6",
    "jasmine-core": "~4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "ngitflow": "^2.0.2",
    "typescript": "^4.9.5"
  }
}
