{
  "name": "ngx-utility",
  "version": "12.0.2-snapshot.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-path ./docs --base-href /ngx-utility/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:core": "ng build core --configuration production",
    "build:files": "ng build files --configuration production",
    "build:dates": "ng build dates --configuration production",
    "publish:core": "npm publish ./dist/core --access public",
    "publish:files": "npm publish ./dist/files --access public",
    "publish:dates": "npm publish ./dist/dates --access public",
    "build:lib": "npm run build:core && npm run build:files && npm run build:dates",
    "update:docs": "npm run build && git add -A docs/ && git commit -m 'Update docs app'",
    "release": "ngitflow release start && npm run update:docs && ngitflow release finish",
    "release:major": "ngitflow release start major && npm run update:docs && ngitflow release finish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "~12.2.1",
    "@angular/compiler": "~12.2.1",
    "@angular/core": "~12.2.1",
    "@angular/forms": "~12.2.1",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "~12.2.1",
    "@angular/platform-browser-dynamic": "~12.2.1",
    "@angular/router": "~12.2.1",
    "highlight.js": "^10.7.3",
    "moment": "^2.29.1",
    "ngx-highlightjs": "^4.1.4",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular-devkit/core": "^12.2.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.1",
    "@angular/language-service": "~12.2.1",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.19",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.0",
    "ngitflow": "^2.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5"
  }
}
